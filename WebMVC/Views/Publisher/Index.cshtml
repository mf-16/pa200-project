@model IEnumerable<WebMVC.Models.Publisher.PublisherViewModel>

<h2>Publishers</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Bio</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var publisher in Model)
        {
            <tr>
                <td>@publisher.Name</td>
                <td>@publisher.Bio</td>
                <td>
                    <a asp-action="Details" asp-route-id="@publisher.Id" class="btn btn-info">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>

